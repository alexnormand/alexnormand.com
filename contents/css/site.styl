* 
  vertical-align: baseline 
  font-family: inherit
  font-style: inherit
  text-decoration:inherit
  font-size: 100%
  padding: 0
  border: 0
  margin: 0


article,aside,details,figcaption,figure,
footer,header,hgroup,menu,nav,section
  display:block


table 
  border-collapse: collapse


::focus 
  outline: 0


::-moz-focus-inner 
  border: 0


.content 
  a 
    text-decoration: none

  p 
    a:hover
      text-decoration: underline



a 
  color: #497CA6
  text-decoration: underline
  -webkit-transition: color ease-in-out .5s
  -moz-transition: color ease-in-out .5s
  -o-transition: color ease-in-out .5s
  -ms-transition: color ease-in-out .5s


  &:hover
    color:#CD3B16


  &:active
    outline: 0
    position: relative
    top: 1px


body
  font-family: "Droid Sans", "Helvetica Neue", Helvetica, Arial, sans-serif
  background-color: #efefef
  padding: 0
  margin: 0
  background-image: url(unquote('data:image/png;base64,'))
   


#container
  margin: auto


footer
  font-size: 10px
  text-align: center
  margin: 20px 0 
  text-shadow: 0px 2px 0px #fff



#appengine 
  display: inline-block
  vertical-align: bottom
  text-indent: -9999px
  width: 35px
  height: 27px
  background-repeat: no-repeat
  background-image: url(unquote('data:image/png;base64,'))
    


.banner
  padding: 0 27px
  position: relative

  h1 
    font-family: Verdana
    font-weight: bold
    font-size: 3.5em
    line-height: 48px
    color: #497CA6
    text-shadow: 0px 1px 2px  #010202    

    a:hover, a
      color: #497CA6
      text-shadow: inherit

  h3
    color: #666
    font-size: 16px
    line-height: 24px
    margin-bottom: 12px


.blog_excerpt 
  padding: 15px 0 24px

  .post time
    float: none
    clear: left

  .read-more
    text-align: right


.blog_excerpt h3,
h1.title
  font-size: 24px
  border-bottom: 1px solid #DEDEDE
  margin: 15px auto

.blog_excerpt h3
  margin-bottom: 0
  font-size: 100%


h1.title
  color: #444
  margin-bottom: 0

section.content
  padding: 30px



#mainmain
  background-color: #F7F7F7
  -moz-box-shadow: 0px 4px 12px #ccc
  -webkit-box-shadow: 0px 4px 12px #ccc
  box-shadow: 0px 4px 12px #ccc


/* Content */

section.content > article.post header
  margin-bottom: 45px


article.post 
  h2
    margin: 30px 0 0 0
    color: #222

  p, ul
    font-size: 15px
    line-height: 22px
    margin: 20px 0
    color: #333

  ul
    margin-left: 20px

  img
    display: block
    margin-bottom: 24px
    box-shadow: 3px 3px 13px #444

.post time
  font-size: 12px
  color: #999
  float:left
  margin-top: 6px


ul.tags
  margin-bottom: 24px
  text-align: right

  li
    list-style-type: none
    display: inline

    a
      color: #999
      text-decoration: none
      
     &:hover
       color: #222

h1 a, h2 a, h3 a
  text-decoration: none


.archives 
  ul.posts
    margin-top: 24px

  li.post
    list-style-type: none
    border-bottom: 1px dotted #ccc
    padding: 12px 0

    time
      color: #999
      text-shadow: 0px 2px 0px #fff
      margin-right: 24px
      display:block
      float: left
      width: 120px

    a
      float: left

  ul.tags
    float: right
    margin-bottom:0


/*===== CUSTOM CSS =====*/
#main 
  width: 800px
  margin: 11px auto


nav.main_nav
  float: right

  ul 
    list-style-type: none
    padding: 9px 15px 15px 15px
    float: left

  li 
    font-weight: bold
    float: left
    margin: 0 20px


select.main_nav 
  display: none
  color: #333
  font-size: 20px
  width: 200px


::selection 
  background: #B51
  color: #EDEDED
  text-shadow: none


::-moz-selection 
  background: #B51
  color: #EDEDED
  text-shadow: none


pre 
  border-radius: 0.3em
  font-size: .9em
  font-family: 'Inconsolata', monospace
  overflow-x: auto
  overflow-y: hidden
  padding: 0.4em
  line-height: 100%
  background-color: #3F3F3F

.hi 
  p 
    font-size: 15px
    line-height: 21px
    margin: 20px 0
    color: #333

  a.btn
    float: right
 

.archive 
  ul 
    list-style: none
    margin: 0
    padding: 5px 20px

  .year
    font-size: 18px
    margin-top: 10px

    ul 
      font-size: 85%
      margin: 0


/*
    webkit scroll bars
*/

::-webkit-scrollbar
  width: 12px
  height: 6px

::-webkit-scrollbar-button:start:decrement,
::-webkit-scrollbar-button:end:increment 
    display: block
    height: 0
    width: 0


::-webkit-scrollbar-button:vertical:increment,
::-webkit-scrollbar-button:horizontal:increment
  background-color: transparent;
  display: block
  height: 0
  width: 0


::-webkit-scrollbar-thumb:vertical 
  background-color: #BFBFBF;


::-webkit-scrollbar-thumb:horizontal
  background-color: #BFBFBF;


pre::-webkit-scrollbar
  width: 6px;


pre::-webkit-scrollbar-track-piece
  -webkit-border-radius: 3px;


pre::-webkit-scrollbar-thumb:vertical 
  -webkit-border-radius: 3px;


pre::-webkit-scrollbar-thumb:horizontal
  -webkit-border-radius: 3px;



@import "zenburn"


/*===============
   MEDIA QUERIES
  ===============*/

@media only screen and (max-width: 800px) {
    #main, img {
	width: 100%;
    }    

    select.main_nav {
	display: inline;
    }

    nav.main_nav {
	display: none;
    }
}

@media print {
    body, a, footer  {
       text-shadow: none;
      }
    
    .banner h1, a {
	color: DarkSlateGrey;
    }

    #mainmain {
	-moz-box-shadow: none; /* FF3.5+ */
	-webkit-box-shadow: none; /* Saf3.0+, Chrome */
	box-shadow: none; /* Opera 10.5, IE 9.0 */
    }    
}
